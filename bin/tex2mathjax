      s,\\BW,\\mathbf{W},g ;
      s,\\BXi\b,\\boldsymbol{\\Xi},g ;
      s,\\BXI\b,\\boldsymbol{\\xi},g ;
      s,\\BX,\\mathbf{X},g ;
      s,\\BY,\\mathbf{Y},g ;
      s,\\BZ,\\mathbf{Z},g ;

      s,\\Hcap\b,\\hat{\\BH},g ;
      s,\\Ecap\b,\\hat{\\BE},g ;
      s,\\acap\b,\\hat{\\Ba},g ;
      s,\\bcap\b,\\hat{\\Bb},g ;
